{
  "expo": {
    "name": "Musicboard",
    "slug": "Musicboard",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.arjit1512.Musicboard"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "SPOTIFY_CLIENT_ID":"ddad339fb6394da18c6e67ef607f189a",
      "SPOTIFY_CLIENT_SECRET":"ace868b377284bbc81c7ffb777721cb9",
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1915b038-624c-49b3-8b07-d0a760e3b586"
      }
    }
  }
}
