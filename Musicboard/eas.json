{
  "cli": {
    "version": ">= 16.0.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "excludeAssets": ["server/**/*"]
    },
    "preview": {
      "distribution": "internal",
      "excludeAssets": ["server/**/*"]
    },
    "production": {
      "autoIncrement": true,
      "excludeAssets": ["server/**/*"]
    }
  },
  "submit": {
    "production": {}
  }
}